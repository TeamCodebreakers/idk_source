{
  "manifest": {
    "publishingInformation": {
      "locales": {
        "en-US": {
          "summary": "Recommend restaurants from Yelp, and texts your group the address and stars.",
          "examplePhrases": [
            "Alexa open feed me now",
            "recommend a place",
            "help"
          ],
          "name": "feed me now",
          "description": "Recommend restaurants from Yelp, and texts your group the address and stars"
        }
      },
      "isAvailableWorldwide": true,
      "testingInstructions": "Alexa open feed me now.",
      "category": "RESTAURANT_BOOKING_INFO_AND_REVIEW",
      "distributionCountries": []
    },
    "apis": {
      "custom": {
        "endpoint": {
          "sourceDir": "lambda/custom",
          "uri": "ask-custom-idk-default"
        }
      }
    },
    "manifestVersion": "1.0",
    "permissions": [
      {
        "name": "alexa::devices:all:address:full:read"
      },
      {
        "name": "alexa::profile:name:read"
      },
      {
        "name": "alexa::profile:mobile_number:read"
      }
    ],
    "privacyAndCompliance": {
      "allowsPurchases": false,
      "locales": {
        "en-US": {
          "termsOfUseUrl": "https://github.com/TeamCodebreakers/IDK/blob/dev/assets/terms.md",
          "privacyPolicyUrl": "https://github.com/TeamCodebreakers/IDK/blob/dev/assets/privacyPolicy.md"
        }
      },
      "isExportCompliant": true,
      "containsAds": false,
      "isChildDirected": false,
      "usesPersonalInfo": false
    }
  }
}
